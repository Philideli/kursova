<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
        <welcome-file>registration.jsp</welcome-file>
    </welcome-file-list>

    <listener>
        <listener-class>com.example.controllers.ContextListener</listener-class>
    </listener>
    
    <servlet>
        <servlet-name>LogInServlet</servlet-name>
        <servlet-class>com.example.controllers.LogInServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogInServlet</servlet-name>
        <url-pattern>/LogInServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AllQueuesServlet</servlet-name>
        <servlet-class>com.example.controllers.AllQueuesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AllQueuesServlet</servlet-name>
        <url-pattern>/AllQueuesServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>UserQueuesServlet</servlet-name>
        <servlet-class>com.example.controllers.UserQueuesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserQueuesServlet</servlet-name>
        <url-pattern>/UserQueuesServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NewQueue</servlet-name>
        <servlet-class>com.example.controllers.NewQueue</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewQueue</servlet-name>
        <url-pattern>/NewQueue</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddToQueue</servlet-name>
        <servlet-class>com.example.controllers.AddToQueue</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddToQueue</servlet-name>
        <url-pattern>/AddToQueue</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>com.example.controllers.LogOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/LogOutServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteFromQueue</servlet-name>
        <servlet-class>com.example.controllers.DeleteFromQueue</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteFromQueue</servlet-name>
        <url-pattern>/DeleteFromQueue</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MuteQueue</servlet-name>
        <servlet-class>com.example.controllers.MuteQueue</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MuteQueue</servlet-name>
        <url-pattern>/MuteQueue</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>NextQueue</servlet-name>
        <servlet-class>com.example.controllers.NextQueue</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NextQueue</servlet-name>
        <url-pattern>/NextQueue</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DetailsQueueServlet</servlet-name>
        <servlet-class>com.example.controllers.DetailsQueueServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DetailsQueueServlet</servlet-name>
        <url-pattern>/DetailsQueueServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NewUser</servlet-name>
        <servlet-class>com.example.controllers.NewUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewUser</servlet-name>
        <url-pattern>/NewUser</url-pattern>
    </servlet-mapping>

</web-app>